{
	"figure": {
		"prefix": "figure",
		"body": [
			"<figure>",
			"<img src=\"${2}\" width=${3} height=${4} alt=\"${5}\">",
			"<figcaption>${1}</figcaption>",
			"</figure>${6}"
		]
	},
