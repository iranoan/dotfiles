		"body": "&rdquo;"
	},
	"span": {
		"prefix": "span",
		"body": "<span ${1:class}=\"${2}\">${3}</span>${4}"
	},
	"ruby": {
		"prefix": "ruby",
		"body": "<ruby>${1}<rp>(</rp><rt>${2}</rt><rp>)</rp></ruby>${3}"
	}
}
