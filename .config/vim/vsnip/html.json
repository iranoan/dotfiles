	"style": {
		"prefix": "style",
		"body": [
			"<style>",
			"\t$1",
			"</style>"
		],
		"description": "HTML - Defines a style definition"
	},
	"style=": {
		"prefix": "style=",
		"body": ["style=\"${1}\""],
		"description": "HTML - style attribution"
	},
	"li": {
