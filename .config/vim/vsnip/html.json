{
	"html5": {
		"prefix": "html5",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"$1ja\">",
			"<head>",
			"\t<meta http-equiv=\"Content-Type\" content=\"text/html;charset=utf-8\">",
			"\t<link rel=\"stylesheet\" type=\"text/css\" href=\"default.css\">",
			"\t<title>$2</title>",
			"</head>",
			"<body>",
			"$0",
			"</body>",
			"</html>"
		],
		"description": "HTML - Defines a template for a html5 document"
	},
	"figure": {
		"prefix": "figure",
		"body": [
			"<figure>",
			"<img src=\"${2}\" width=${3} height=${4} alt=\"${5}\">",
			"<figcaption>${1}</figcaption>",
			"</figure>${6}"
		]
	},
	"table": {
		"prefix": "table",
		"body": [
			"<table>",
			"<caption>${1}</caption>",
			"<tr><th>${2}</th>${3}</tr>",
			"${4}",
			"</table>${5}"
		]
	},
	"tr": {
		"prefix": "tr",
		"body": "<tr>${1}</tr>"
	},
	"th": {
		"prefix": "th",
		"body": "<th abbr=\"${1}\">${2}</th>${3}"
	},
	"td": {
		"prefix": "td",
		"body": "<td>${1}</td>${2}"
	},
	"h1": {
		"prefix": "h1",
		"body": [
			"<h1>${1}</h1>",
			"${2}"
		]
	},
	"h2": {
		"prefix": "h2",
